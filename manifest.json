{
  "update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Bootstrap Overlay",
  "description": "This extension creates an overlay for the bootstrap grid system",
  "version": "2.1",
  "icons": { "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" },
    "browser_action": {
      "default_icon": "icon32_off.png"
    },
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": [
      "tabs", "http://*/*", "https://*/*"
    ]
}
